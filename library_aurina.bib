title = {{Sensory Neurons Arouse C. elegans Locomotion via Both Glutamate and Neuropeptide Release.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26154367 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4495980},
