journal = {Multimodal brain image analysis : second International Workshop, MBIA 2012, held in conjunction with MICCAI 2012, Nice, France, October 1-5, 2012 : proceedings / Pew-Thian Yap ... [et al.], (eds.). MBIA (Workshop) (2nd : 2012 : Nice, Fr...},
keywords = {Dijkstra's algorithm,HARDI tractography,Structural connectivity,neuroimaging genetics,path length},
pages = {29--40},
