issn = {1945-7928},
journal = {Proceedings / IEEE International Symposium on Biomedical Imaging: from nano to macro},
pages = {542},
pmid = {22903411},
publisher = {NIH Public Access},
title = {{Discovery of genes that affect human brain connectivity: a genome-wide analysis of the connectome}},
