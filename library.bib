keywords = {Anisotropy,Brain,Brain: anatomy {\&} histology,Computer-Assisted,Diffusion Magnetic Resonance Imaging,Image Processing},
month = {jul},
number = {3},
pages = {316--29},
pmid = {17599699},
title = {{Diffusion tensor imaging of the brain.}},
