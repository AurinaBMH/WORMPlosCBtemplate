keywords = {Adult,Brain Mapping,Brain Mapping: methods,Cerebral Cortex,Cerebral Cortex: physiology,Dizygotic,Dizygotic: genetics,Female,Humans,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Male,Middle Aged,Monozygotic,Monozygotic: genetics,Nerve Net,Nerve Net: physiology,Twins,Young Adult},
month = {mar},
