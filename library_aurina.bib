keywords = {Aged,Axons,Axons: pathology,Diffusion Magnetic Resonance Imaging,Female,Humans,Magnetic Resonance Imaging,Male,Multiple Sclerosis,Multiple Sclerosis: pathology,Multiple Sclerosis: radiography,Myelin Sheath,Myelin Sheath: pathology,Spinal Cord,Spinal Cord: pathology,Spinal Cord: radiography},
month = {nov},
