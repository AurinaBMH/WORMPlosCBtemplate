issn = {1750-2799},
journal = {Nature protocols},
keywords = {Animals,Case-Control Studies,Computational Biology,Computational Biology: methods,Female,Genetic Techniques,Genome-Wide Association Study,Genome-Wide Association Study: methods,Humans,Male,Quality Control},
number = {9},
pages = {1564--73},
pmid = {21085122},
title = {{Data quality control in genetic case-control association studies}},
url = {http://www.nature.com/nprot/journal/v5/n9/abs/nprot.2010.116.html{\%}5Cnhttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3025522{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {5},
