pages = {322--36},
pmid = {23531697},
title = {{Imaging structural co-variance between human brain regions.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23531697 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4043276},
volume = {14},
year = {2013}
}
