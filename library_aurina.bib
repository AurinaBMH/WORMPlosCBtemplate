primaryClass = {arXiv:cs},
title = {{The Rich-Club Phenomenon In The Internet Topology}},
year = {2003}
}
@article{Mcauley2007,
abstract = {The " rich-club phenomenon " in complex networks is characterized when nodes of higher degree are more interconnected than nodes with lower degree. The presence of this phenomenon may indicate several interesting high-level network properties, such as tolerance to hub failures. Here, the authors investigate the existence of this phenomenon across the hierarchies of several real-world networks. Their simulations reveal that the presence or absence of this phenomenon in a network does not imply its presence or absence in the network's successive hierarchies, and that this behavior is even nonmonotonic in some cases. The so-called rich-club phenomenon in complex net-works is characterized when the hubs ͑i.e., nodes with high degrees͒ are on average more intensely interconnected than the nodes with smaller degrees. More precisely, it happens when the nodes with degree larger than k tend to be more densely connected among themselves than the nodes with degree smaller than k, for some significant range of degrees in the network.},
author = {Mcauley, Julian J and {Da Fontoura Costa}, Luciano and Caetano, Tib{\'{e}}rio S},
doi = {10.1063/1.2773951͔},
file = {:Users/Aurina/Library/Application Support/Mendeley Desktop/Downloaded/Mcauley, Da Fontoura Costa, Caetano - 2007 - Rich-club phenomenon across complex network hierarchies.pdf:pdf},
title = {{Rich-club phenomenon across complex network hierarchies}},
year = {2007}
}
