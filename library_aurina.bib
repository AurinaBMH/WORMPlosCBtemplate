pages = {345--57},
pmid = {9292724},
title = {{Regulation of interneuron function in the C. elegans thermoregulatory pathway by the ttx-3 LIM homeobox gene.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/9292724},
volume = {19},
