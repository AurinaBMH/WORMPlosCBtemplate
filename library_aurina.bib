issn = {1742-5662},
journal = {Journal of the Royal Society, Interface / the Royal Society},
keywords = {Data Interpretation,Electroencephalography,Heart Rate,Models,Research Design,Speech Articulation Tests,Statistical,Theoretical,Time},
