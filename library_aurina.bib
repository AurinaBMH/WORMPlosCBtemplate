@article{Hobert2016,
abstract = {We revisit the classification of neuronal cell types in the nervous system of the nematode Caenorhabditis elegans. Based on anatomy and synaptic connectivity patterns, the 302 neurons of the nervous system of the hermaphrodite were categorized into 118 neuron classes more than 30 years ago. Analysis of all presently available neuronal gene expression patterns reveals a remarkable congruence of anatomical and molecular classification and further suggests subclassification schemes. Transcription factor expression profiles alone are sufficient to uniquely classify more than 90{\%} of all neuron classes in the C. elegans nervous system. Neuron classification in C. elegans may be paradigmatic for neuron classification schemes in vertebrate nervous systems.},
author = {Hobert, Oliver and Glenwinkel, Lori and White, John},
doi = {10.1016/j.cub.2016.10.027},
