@article{Daianu2014,
abstract = {Diffusion imaging and brain connectivity analyses can reveal the underlying organizational patterns of the human brain, described as complex networks of densely interlinked regions. Here, we analyzed 1.5-Tesla whole-brain diffusion-weighted images from 64 participants - 15 patients with behavioral variant frontotemporal (bvFTD) dementia, 19 with early-onset Alzheimer's disease (EOAD), and 30 healthy elderly controls. Based on whole-brain tractography, we reconstructed structural brain connectivity networks to map connections between cortical regions. We examined how bvFTD and EOAD disrupt the weighted 'rich club' - a network property where high-degree network nodes are more interconnected than expected by chance. bvFTD disrupts both the nodal and global organization of the network in both low- and high-degree regions of the brain. EOAD targets the global connectivity of the brain, mainly affecting the fiber density of high-degree (highly connected) regions that form the rich club network. These rich club analyses suggest distinct patterns of disruptions among different forms of dementia.},
author = {Daianu, Madelaine and Jahanshad, Neda and Villalon-Reina, Julio E and Mendez, Mario F and Bartzokis, George and Jimenez, Elvira E and Joshi, Aditi and Barsuglia, Joseph and Thompson, Paul M},
doi = {10.1007/978-3-319-11182-7_2},
file = {:Users/Aurina/Library/Application Support/Mendeley Desktop/Downloaded/Daianu et al. - 2014 - Rich club network analysis shows distinct patterns of disruption in frontotemporal dementia and Alzheimer's disea.pdf:pdf},
issn = {1612-3786},
journal = {Mathematics and visualization},
pages = {13--22},
pmid = {26161050},
publisher = {NIH Public Access},
title = {{Rich club network analysis shows distinct patterns of disruption in frontotemporal dementia and Alzheimer's disease.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26161050 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4492471},
volume = {2014},
year = {2014}
}
@article{Park2013,
abstract = {How rich functionality emerges from the invariant structural architecture of the brain remains a major mystery in neuroscience. Recent applications of network theory and theoretical neuroscience to large-scale brain networks have started to dissolve this mystery. Network analyses suggest that hierarchical modular brain networks are particularly suited to facilitate local (segregated) neuronal operations and the global integration of segregated functions. Although functional networks are constrained by structural connections, context-sensitive integration during cognition tasks necessarily entails a divergence between structural and functional networks. This degenerate (many-to-one) function-structure mapping is crucial for understanding the nature of brain networks. The emergence of dynamic functional networks from static structural connections calls for a formal (computational) approach to neuronal information processing that may resolve this dialectic between structure and function.},
