pmid = {17122850},
title = {{Global variation in copy number in the human genome.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17122850 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2669898},
volume = {444},
year = {2006}
}
@article{Kloosterman2015,
abstract = {Small insertions and deletions (indels) and large structural variations (SVs) are major contributors to human genetic diversity and disease. However, mutation rates and characteristics of de novo indels and SVs in the general population have remained largely unexplored. We report 332 validated de novo structural changes identified in whole genomes of 250 families, including complex indels, retrotransposon insertions, and interchromosomal events. These data indicate a mutation rate of 2.94 indels (1-20 bp) and 0.16 SVs ({\textgreater}20 bp) per generation. De novo structural changes affect on average 4.1 kbp of genomic sequence and 29 coding bases per generation, which is 91 and 52 times more nucleotides than de novo substitutions, respectively. This contrasts with the equal genomic footprint of inherited SVs and substitutions. An excess of structural changes originated on paternal haplotypes. Additionally, we observed a nonuniform distribution of de novo SVs across offspring. These results reveal the importance of different mutational mechanisms to changes in human genome structure across generations.},
author = {Kloosterman, Wigard P and Francioli, Laurent C and Hormozdiari, Fereydoun and Marschall, Tobias and Hehir-Kwa, Jayne Y and Abdellaoui, Abdel and Lameijer, Eric-Wubbo and Moed, Matthijs H and Koval, Vyacheslav and Renkens, Ivo and van Roosmalen, Markus J and Arp, Pascal and Karssen, Lennart C and Coe, Bradley P and Handsaker, Robert E and Suchiman, Eka D and Cuppen, Edwin and Thung, Djie Tjwan and McVey, Mitch and Wendl, Michael C and {Genome of Netherlands Consortium} and Uitterlinden, Andr{\'{e}} and van Duijn, Cornelia M and Swertz, Morris A and Wijmenga, Cisca and van Ommen, GertJan B and Slagboom, P Eline and Boomsma, Dorret I and Sch{\"{o}}nhuth, Alexander and Eichler, Evan E and de Bakker, Paul I W and Ye, Kai and Guryev, Victor},
doi = {10.1101/gr.185041.114},
issn = {1549-5469},
journal = {Genome research},
month = {jun},
number = {6},
pages = {792--801},
pmid = {25883321},
title = {{Characteristics of de novo structural changes in the human genome.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25883321 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4448676},
volume = {25},
year = {2015}
}
@article{Pendleton2015,
abstract = {We present the first comprehensive analysis of a diploid human genome that combines single-molecule sequencing with single-molecule genome maps. Our hybrid assembly markedly improves upon the contiguity observed from traditional shotgun sequencing approaches, with scaffold N50 values approaching 30 Mb, and we identified complex structural variants (SVs) missed by other high-throughput approaches. Furthermore, by combining Illumina short-read data with long reads, we phased both single-nucleotide variants and SVs, generating haplotypes with over 99{\%} consistency with previous trio-based studies. Our work shows that it is now possible to integrate single-molecule and high-throughput sequence data to generate de novo assembled genomes that approach reference quality.},
author = {Pendleton, Matthew and Sebra, Robert and Pang, Andy Wing Chun and Ummat, Ajay and Franzen, Oscar and Rausch, Tobias and St{\"{u}}tz, Adrian M and Stedman, William and Anantharaman, Thomas and Hastie, Alex and Dai, Heng and Fritz, Markus Hsi-Yang and Cao, Han and Cohain, Ariella and Deikus, Gintaras and Durrett, Russell E and Blanchard, Scott C and Altman, Roger and Chin, Chen-Shan and Guo, Yan and Paxinos, Ellen E and Korbel, Jan O and Darnell, Robert B and McCombie, W Richard and Kwok, Pui-Yan and Mason, Christopher E and Schadt, Eric E and Bashir, Ali},
